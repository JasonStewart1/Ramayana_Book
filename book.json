{
  
    // Book title and description (defaults are extracted from the README)
    "title": "Ramayana-book",
    "description": "Book about Ramayana ( One of the two great epics of Hinduism ) ascribed by Valmiki consists of 24,000 verses in seven books (kāṇḍas) and 500 cantos (sargas)",

    // GitHub information (defaults are extracted using git)
    "github": "https://github.com/svenkatreddy/Ramayana_Book",
    "githubHost": "http://svenkatreddy.github.io/Ramayana_Book/",

    // Links in template (null: default, false: remove, string: new value)
    "links": {
        // Link to home in the top-left corner
        "home": "http://svenkatreddy.github.io/Ramayana_Book/",

        // Links in top of sidebar
        "about": "http://svenkatreddy.github.io/Ramayana_Book/san_book/",
        "issues": "https://github.com/svenkatreddy/Ramayana_Book/issues",
        "contribute": "https://github.com/svenkatreddy/Ramayana_Book/",

        // Sharing links
        "sharing": {
            "google": "http://svenkatreddy.github.io/Ramayana_Book/",
            "facebook": "http://svenkatreddy.github.io/Ramayana_Book/",
            "twitter": "http://svenkatreddy.github.io/Ramayana_Book/"
        }
    },


    // Options for PDF generation
    "pdf": {
        // Add toc at the end of the file
        "toc": true,

        // Add page numbers to the bottom of every page
        "pageNumbers": true,

        // Font size for the fiel content
        "fontSize": 12,

        // Paper size for the pdf
        // Choices are [u’a0’, u’a1’, u’a2’, u’a3’, u’a4’, u’a5’, u’a6’, u’b0’, u’b1’, u’b2’, u’b3’, u’b4’, u’b5’, u’b6’, u’legal’, u’letter’]
        "paperSize": "a4",

        // Margin (in pts)
        // Note: 72 pts equals 1 inch
        "margin": {
            "right": 62,
            "left": 62,
            "top": 36,
            "bottom": 36
        }
    }
}
